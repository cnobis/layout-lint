{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "layout-lint",
      "camelcase": "LayoutLint",
      "title": "Layout Lint",
      "scope": "source.layout-lint",
      "file-types": [
        "layoutlint",
        "layout-lint"
      ],
      "injection-regex": "^layout-lint$",
      "class-name": "TreeSitterLayoutLint"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Layout Lint grammar for tree-sitter",
    "authors": [
      {
        "name": "Christopher Nobis"
      }
    ],
    "links": {
      "repository": "https://github.com/cnobis/layout-lint.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
